{
	"schemaVersion": 1,

	"id": "nobrainclient",

	"version": "${version}",

	"name": "NoBrainClient",

	"description": "We are an open Minecraft client aiming to add many cool features, such as emotes, name tags, nicknames, a friends system, and world hosting.",

	"authors": [
		"TheNoBrainGuy"
	],

	"contributors" : [
		"GODZILLA",
		"sourcecode.yml",
		"TheDeamonLord333"
	],

	"contact": {
		"homepage": "https://discord.gg/4fkfqAhwN3",
		"sources": "https://github.com/thenobrainguyy/nobrainclient",
		"issues": "https://github.com/thenobrainguyy/issues/issues"
	},

	"license": "CC0-1.0",

	"icon": "assets/nobrainclient/nobrainclient-icon.png",

	"environment": "*",

	"entrypoints": {
		"main": [
			"com.thenobrainguy.nobrainclient.NoBrainClient"
		],
		"client": [
			"com.thenobrainguy.nobrainclient.NoBrainClientClient"
		]
	},

	"mixins": [
		"nobrainclient.mixins.json",
		{
			"config": "nobrainclient.client.mixins.json",
			"environment": "client"
		}
	],

	"depends": {
		"fabricloader": ">=0.16.10",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*"
	},

	"custom" : {
		"fabric-renderer-api-v1:contains_renderer" : true,
		"modmenu" : {
			"links" : {
				"modmenu.discord" : "https://discord.gg/4fkfqAhwN3",
				"modmenu.modrinth" : "https://modrinth.com/"
			}
		}
	},

	"breaks" : {
		"cactus" : "*",
		"essential-container": "*"
	},

	"suggests": {
		"another-mod": "*"
	}

}